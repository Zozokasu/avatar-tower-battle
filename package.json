{
  "name": "mirage-x-template",
  "scripts": {
    "start": "node ./dist/server/src/index.js",
    "build": "run-p build:*",
    "build:web": "webpack --config ./src/web/webpack.config.ts",
    "build:neos": "tsc --project ./src/neos/tsconfig.json && node ./dist/neos/src/index.js",
    "build:server": "tsc --project ./src/server/tsconfig.json",
    "dev": "run-p dev:*",
    "dev:web": "webpack --watch --mode production --config ./src/web/webpack.config.ts",
    "dev:neos": "tsc-watch --project ./src/neos/tsconfig.json --onSuccess \"node ./dist/neos/src/index.js\"",
    "dev:server": "tsc-watch --project ./src/server/tsconfig.json --onSuccess \"npm start\"",
    "feedback:fetch": "ts-node ./src/neosFeedback/fetch",
    "feedback:attach:base": "ts-node ./src/neosFeedback/attach",
    "feedback:attach:unit": "ts-node ./src/neosFeedback/attachUnit"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@emotion/babel-preset-css-prop": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/uuid": "^9.0.1",
    "@types/ws": "^8.5.4",
    "babel-loader": "^9.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.0.4",
    "webpack": "^5.84.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "7zbson.js": "^1.0.2",
    "axios": "^1.4.0",
    "express": "^4.18.2",
    "jsdom": "^22.0.0",
    "json2emap": "^0.2.0",
    "neos-script": "^0.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.1.5",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  }
}
